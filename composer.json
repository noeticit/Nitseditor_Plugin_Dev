{
    "name": "noeticitservices/plugindev",
    "description": "Nitseditor Plugin development environment",
    "license": "MIT",
    "authors": [
        {
            "name": "Nitish Kumar",
            "email": "nitishkumar@noeticitservices.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "5.7.*",
        "illuminate/console": "5.7.*",
        "illuminate/routing": "5.7.*",
        "illuminate/database": "5.7.*",
        "laravel/passport": "^7.0",
        "wikimedia/composer-merge-plugin": "dev-master"
    },
    "autoload": {
        "files": [
            "src/System/Helpers/helpers.php"
        ],
        "classmap": [
            "src/System/Database"
        ],
        "psr-4": {
            "Nitseditor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nitseditor\\System\\Providers\\NitsEditorServiceProvider"
            ],
            "aliases": {

            }
        },
        "merge-plugin": {
            "include": [
                "composer.local.json",
                "plugins/*/composer.json"
            ],
            "require": [
                "submodule/composer.json"
            ],
            "recurse": true,
            "replace": false,
            "ignore-duplicates": false,
            "merge-dev": true,
            "merge-extra": false,
            "merge-extra-deep": false,
            "merge-scripts": false
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
